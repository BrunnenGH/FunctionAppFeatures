{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "Default": "Trace",
      "Function.Function1.User": "Debug",
      "Function.Function2.User": "Information",
      "MyLibrary.MyLibrary.Class1": "Trace",
      "MyLibrary.MyLibrary": "Trace",
      "MyLibrary": "Trace",
      "Host.Results": "Information",
      "Host.Aggregator": "Information"
    },
    "applicationInsights": { "samplingSettings": { "EnableDependencyTracking": true } },
    "console": {
      "isEnabled": false,
      "IncludeScopes": true
    }
  }
}